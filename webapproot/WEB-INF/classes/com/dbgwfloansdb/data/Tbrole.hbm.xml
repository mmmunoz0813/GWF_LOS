<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbgwfloansdb.data.Tbrole" table="tbrole" catalog="dbgwfloans" dynamic-insert="false" dynamic-update="false">
        <id name="roleId" type="integer">
            <column name="roleId"/>
            <generator class="identity"/>
        </id>
        <property name="rolename" type="string">
            <column name="rolename" length="20" not-null="true"/>
        </property>
        <property name="roledesc" type="string">
            <column name="roledesc" length="100"/>
        </property>
    </class>
</hibernate-mapping>