<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbgwfloansdb.data.Tbcodes" table="tbcodes" catalog="dbgwfloans" dynamic-insert="false" dynamic-update="false">
        <id name="codeId" type="integer">
            <column name="codeId"/>
            <generator class="identity"/>
        </id>
        <property name="codename" type="string">
            <column name="codename" length="20" not-null="true"/>
        </property>
        <property name="codevalue" type="string">
            <column name="codevalue" length="20" not-null="true"/>
        </property>
        <property name="codedesc1" type="string">
            <column name="codedesc1" length="100" not-null="true"/>
        </property>
        <property name="codedesc2" type="string">
            <column name="codedesc2" length="100"/>
        </property>
        <property name="remarks" type="string">
            <column name="remarks" length="100"/>
        </property>
    </class>
</hibernate-mapping>