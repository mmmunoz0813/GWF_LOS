<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbgwfloansdb.data.Tbuser" table="tbuser" catalog="dbgwfloans" dynamic-insert="false" dynamic-update="false">
        <id name="userId" type="integer">
            <column name="userId"/>
            <generator class="identity"/>
        </id>
        <property name="username" type="string">
            <column name="username" length="10" not-null="true"/>
        </property>
        <property name="password" type="string">
            <column name="password" length="20" not-null="true"/>
        </property>
        <property name="lastname" type="string">
            <column name="lastname" length="50" not-null="true"/>
        </property>
        <property name="firstname" type="string">
            <column name="firstname" length="50" not-null="true"/>
        </property>
        <property name="middlename" type="string">
            <column name="middlename" length="50"/>
        </property>
        <property name="role" type="string">
            <column name="role" length="20"/>
        </property>
        <property name="status" type="string">
            <column name="status" length="2"/>
        </property>
        <property name="expirydate" type="date">
            <column name="expirydate" length="10"/>
        </property>
        <property name="lastlogindate" type="date">
            <column name="lastlogindate" length="10"/>
        </property>
        <property name="branch" type="string">
            <column name="branch" length="5"/>
        </property>
    </class>
</hibernate-mapping>