<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<service xmlns="http://www.activegrid.com/namespaces/ServiceDefinitions/1.0" id="UserFacade" class="com.los.user.UserFacade" CRUDService="false" type="JavaService" springFile="UserFacade.spring.xml">
    <operation name="getRoles">
        <return typeRef="com.dbgwfloansdb.data.Tbrole" isList="true"/>
    </operation>
    <operation name="getUsers">
        <parameter name="branch" typeRef="java.lang.String" isList="false"/>
        <return typeRef="com.dbgwfloansdb.data.Tbuser" isList="true"/>
    </operation>
    <operation name="sampleJavaOperation">
        <return typeRef="java.lang.String" isList="false"/>
    </operation>
    <operation name="saveUser">
        <parameter name="id" typeRef="java.lang.String" isList="false"/>
        <parameter name="username" typeRef="java.lang.String" isList="false"/>
        <parameter name="password" typeRef="java.lang.String" isList="false"/>
        <parameter name="lname" typeRef="java.lang.String" isList="false"/>
        <parameter name="fname" typeRef="java.lang.String" isList="false"/>
        <parameter name="mname" typeRef="java.lang.String" isList="false"/>
        <parameter name="expirydate" typeRef="java.util.Date" isList="false"/>
        <parameter name="lastlogindate" typeRef="java.util.Date" isList="false"/>
        <parameter name="branch" typeRef="java.lang.String" isList="false"/>
        <parameter name="status" typeRef="java.lang.String" isList="false"/>
        <parameter name="role" typeRef="java.lang.String" isList="false"/>
    </operation>
    <dataobjects>
        <dataobject javaType="java.util.Date" name="Date" supportsQuickData="false">
            <element name="calendarDate" typeRef="sun.util.calendar.BaseCalendar.Date" isList="false" allowNull="false"/>
            <element name="date" typeRef="int" isList="false" allowNull="false"/>
            <element name="day" typeRef="int" isList="false" allowNull="false"/>
            <element name="hours" typeRef="int" isList="false" allowNull="false"/>
            <element name="julianCalendar" typeRef="sun.util.calendar.BaseCalendar" isList="false" allowNull="false"/>
            <element name="minutes" typeRef="int" isList="false" allowNull="false"/>
            <element name="month" typeRef="int" isList="false" allowNull="false"/>
            <element name="seconds" typeRef="int" isList="false" allowNull="false"/>
            <element name="time" typeRef="long" isList="false" allowNull="false"/>
            <element name="timeImpl" typeRef="long" isList="false" allowNull="false"/>
            <element name="timezoneOffset" typeRef="int" isList="false" allowNull="false"/>
            <element name="year" typeRef="int" isList="false" allowNull="false"/>
        </dataobject>
    </dataobjects>
</service>
