<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbgwfloansdb.data.Tbdocuments" table="tbdocuments" catalog="dbgwfloans" dynamic-insert="false" dynamic-update="false">
        <id name="docId" type="integer">
            <column name="docId"/>
            <generator class="identity"/>
        </id>
        <property name="document" type="string">
            <column name="document" length="50" not-null="true"/>
        </property>
        <property name="remarks" type="string">
            <column name="remarks" length="100"/>
        </property>
    </class>
</hibernate-mapping>